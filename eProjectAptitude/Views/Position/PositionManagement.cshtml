@model IEnumerable<eProjectAptitude.Models.Entity.Position>

@{
    ViewBag.Title = "PositionManagement";
    Layout = "~/Views/Home/_MyLayout.cshtml";
}

<h2>PositionManagement</h2>

<p>
    @Html.ActionLink("Create New", "AddPosition")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PositionName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PositionName)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditPosition", new { id=item.ID }) |
            @Html.ActionLink("Details", "DetailPosition", new { id=item.ID }) |
            @Html.ActionLink("Delete", "DeletePosition", new { id=item.ID })
        </td>
    </tr>
}

</table>
